    <top>
        <Label minHeight="250" />
    </top>
    <left>
        <Label minWidth="250" />
    </left>
    <center>
        <VBox>
            <Label fx:id="endOfGameText" prefWidth="400" text="You Win!!">
            <font>
               <Font name="System Bold" size="18.0" />
            </font></Label>
            <HBox>
